{"ts":1367267734692,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"<!DOCTYPE html>\n<html>\n  <head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Find a route using Geolocation and Google Maps API</title>\n    <link rel=\"stylesheet\" href=\"stylesheets/base.css\">\n    <link rel=\"stylesheet\" href=\"stylesheets/skeleton.css\">\n    <link rel=\"stylesheet\" href=\"stylesheets/layout.css\">\n    <script type=\"text/javascript\" src=\"//maps.google.com/maps/api/js?sensor=true\"></script>\n    <script type=\"text/javascript\" src=\"//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js\"></script>\n    <script type=\"text/javascript\" src=\"markerscript.js\"></script>    \n  </head>\n  <body>\n  \n<div class=\"container\">\n\n<div class=\"row\">\n  <div class=\"sixteen columns\">\n      <h3>Plotting evenly-spaced markers along a Google Maps polyline route</h3>\n  </div>\n</div>\n\n<div class=\"row\">\n  <div class=\"five columns\">\n    <form id=\"calculate-route\" name=\"calculate-route\" action=\"#\" method=\"get\">\n      \n      <label for=\"from\">From:</label>\n      <input type=\"text\" id=\"from\" name=\"from\" required=\"required\" value=\"New York, NY\" size=\"30\">\n      <a class=\"hide\" id=\"from-link\" href=\"#\">Get my position</a>\n   \n      <label for=\"to\">To:</label>\n      <input type=\"text\" id=\"to\" name=\"to\" required=\"required\" value=\"Chicago, IL\" size=\"30\">\n      <a class=\"hide\" id=\"to-link\" href=\"#\">Get my position</a>\n       <label for=\"unitsWanted\">Units:</label>\n     \n     <select id=\"unitsWanted\">\n      <option value=\"Imperial\">Miles</option>\n      <option value=\"Metric\">Kilometers</option>\n     </select>   \n      \n    <span class=\"hide\">\n      <label for=\"segmentsWanted\">Number of markers to show:</label>\n      <input type=\"number\" id=\"segmentsWanted\" name=\"segmentsWanted\" required=\"required\" value=\"30\" size=\"30\">\n     \n      <label for=\"markerTypeWanted\">Marker style:</label>\n      <select id=\"markerTypeWanted\">\n        <option value=\"Numbered\">Numbered</option>\n        <option value=\"Standard\">Standard</option>\n      </select>\n    </span>\n    \n    <br>\n     \n      <input type=\"submit\">\n      <input type=\"reset\">\n\n    </form>\n  </div>\n  <div class=\"eleven columns\">\n    <div id=\"map_canvas\" class=\"scale-with-grid\"></div>\n    <p id=\"error\"></p>\n  </div>\n</div>\n</body>\n</html>"]],"start1":0,"start2":0,"length1":0,"length2":2256}]],"length":2256}
